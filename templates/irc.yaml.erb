---
:server: '<%= @server %>'
<% if @server_password -%>
:server_password: '<%= @server_password %>'
<% end -%>
<% if @port -%>
:port: '<%= @port %>'
<% end -%>
<% if @ssl -%>
:ssl: <%= @ssl %>
<% end -%>
:nick: '<%= @nick %>'
:channel: '<%= @channel %>'
<% if @channel_password -%>
:channel_password: '<%= @channel_password %>'
<% end -%>
<% if @github_user -%>
:github_user: '<%= @github_user %>'
<% end -%>
<% if @github_password -%>
:github_password: '<%= @github_password %>'
<% end -%>
